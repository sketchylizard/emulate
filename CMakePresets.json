{
  "version": 6,
  "configurePresets": [
    {
      "name": "clang-debug",
      "generator": "Ninja",
      "binaryDir": "build/clang/debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/build/clang/debug/conan_toolchain.cmake"
      }
    },
    {
      "name": "clang-release",
      "generator": "Ninja",
      "binaryDir": "build/clang/release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/build/clang/release/conan_toolchain.cmake"
      }
    },
    {
      "name": "gcc-debug",
      "generator": "Ninja",
      "binaryDir": "build/gcc/debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/build/gcc/debug/build/Debug/generators/conan_toolchain.cmake"
      }
    },
    {
      "name": "gcc-release",
      "generator": "Ninja",
      "binaryDir": "build/gcc/release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/build/gcc/release/build/Release/generators/conan_toolchain.cmake"
      }
    },
    {
      "name": "msvc-debug",
      "generator": "Ninja",
      "binaryDir": "build/msvc/debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/build/msvc/debug/build/Debug/generators/conan_toolchain.cmake"
      }
    },
    {
      "name": "msvc-release",
      "generator": "Ninja",
      "binaryDir": "build/msvc/release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/build/msvc/release/build/Release/generators/conan_toolchain.cmake"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "clang-debug",
      "configurePreset": "clang-debug"
    },
    {
      "name": "clang-release",
      "configurePreset": "clang-release"
    },
    {
      "name": "gcc-debug",
      "configurePreset": "gcc-debug"
    },
    {
      "name": "gcc-release",
      "configurePreset": "gcc-release"
    },
    {
      "name": "msvc-debug",
      "configurePreset": "msvc-debug"
    },
    {
      "name": "msvc-release",
      "configurePreset": "msvc-release"
    }
  ]
}
